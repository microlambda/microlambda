{
  "name": "serverless-microlambda",
  "version": "0.3.0-alpha.1",
  "engines": {
    "node": ">=14.0"
  },
  "description": "Serverless plugin for microservices architecture.",
  "author": "Mario Arnautou",
  "license": "MIT",
  "keywords": [
    "serverless plugin microservices",
    "microlambda",
    "serverless plugins",
    "api gateway",
    "lambda",
    "aws",
    "aws lambda",
    "amazon",
    "microservices",
    "amazon web services",
    "serverless.com"
  ],
  "main": "dist/index.js",
  "scripts": {
    "lint": "../node_modules/.bin/eslint '{src,test}/**/*.{ts,tsx,json,yml}'",
    "prettier": "../node_modules/.bin/prettier --write '{src,test}/**/*.{ts,tsx,json,yml}'",
    "build": "../node_modules/.bin/tsc --build",
    "test": "../node_modules/.bin/jest"
  },
  "files": [
    "dist/**/*.js"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-acm": "^3.42.0",
    "@aws-sdk/client-apigatewayv2": "^3.42.0",
    "@aws-sdk/client-cloudformation": "^3.42.0",
    "@aws-sdk/client-lambda": "^3.42.0",
    "@aws-sdk/client-route-53": "^3.42.0",
    "@aws-sdk/client-s3": "^3.42.0",
    "@aws-sdk/client-secrets-manager": "^3.42.0",
    "@microlambda/core": "workspace:*",
    "chokidar": "^3.5.2",
    "hasha": "^5.2.2",
    "joi": "^17.4.2"
  }
